<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	
	<typeAliases>
		<typeAlias type="com.green.biz.product.dto.ProductVO" alias="product"></typeAlias>
		<typeAlias type="com.green.biz.member.MemberVO" alias="member"></typeAlias>
		<typeAlias type="com.green.biz.address.AddressVO" alias="address"></typeAlias>
		<typeAlias type="com.green.biz.order.CartVO" alias="cart"></typeAlias>
		<typeAlias type="com.green.biz.order.OrderVO" alias="order"></typeAlias>
		<typeAlias type="com.green.biz.qna.QnaVO" alias="qna"></typeAlias>
		<typeAlias type="com.green.biz.worker.WorkerVO" alias="worker"></typeAlias>
		<typeAlias type="com.green.biz.product.ProductCommentVO" alias="comment"/>
		<typeAlias type="com.green.biz.product.SalesQuantity" alias="salesMap"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mappings/product-mapping.xml"/>
		<mapper resource="mappings/member-mapping.xml"></mapper>
		<mapper resource="mappings/address-mapping.xml"></mapper>
		<mapper resource="mappings/cart-mapping.xml"></mapper>
		<mapper resource="mappings/order-mapping.xml"></mapper>
		<mapper resource="mappings/qna-mapping.xml"></mapper>
		<mapper resource="mappings/admin-mapping.xml"></mapper>
		<mapper resource="mappings/comment-mapping.xml"></mapper>
	</mappers>
	
</configuration>

